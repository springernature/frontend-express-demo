{
  "name": "frontend-techtest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "node --experimental-modules build.mjs",
    "dev": "nodemon -e js,hbs,scss --ignore public/javascripts --exec 'npm run build && DEBUG=:* & npm start'",
    "test": "npx jest --verbose tests/javascripts/**/*.test.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babel-jest": "^25.4.0",
    "babel-preset-es2015": "^6.24.1",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "core-js": "^3.6.5",
    "eslint": "^7.7.0",
    "eslint-config-google": "^0.14.0",
    "fs-extra": "^9.0.1",
    "jest-cli": "^26.4.2",
    "jest-handlebars": "^1.0.1",
    "nodemon": "^2.0.4",
    "node-sass": "^4.14.1",
    "rollup": "^2.26.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^6.1.0",
    "stylelint": "^13.3.3"
  },
  "jest": {
    "testURL": "http://localhost:3001",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.hbs$": "jest-handlebars"
    },
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "public"
    ],
    "setupFiles": []
  }
}
